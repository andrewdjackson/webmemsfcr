{% set excludes = ['Time', '7d ECU Response', '80 ECU Response', '80x04 Ambient (°C)', '80x06 Fuel °C', '80x0b Air Con. Switch'] %}

<div class="text-start" id="ecuid">ECU ID:</div>

<div class="row align-items-start justify-content-evenly text-start">
	<div class="col">
		<table class="table table-striped table-sm">
			<tbody id="df80table">
			{% for item in data["ecudata_df80.json"]["ecudata"] %}
				{% if item["name"] not in excludes %}
				<tr>
					<td>{{ item['name'] | safe }}</td>
					<td class="ecudata_{{ item['id'] | safe }}">&nbsp;</td>
					<!--
					<td style="width: 30%"><div class="spark-container"><canvas id="{{ item['id'] | safe }}_ecuspark" title="{{ item['name'] | safe }}" class="ecuspark" height="30"></canvas></div></td>
					-->
				</tr>
				{% endif %}
			{% endfor %}
			</tbody>
		</table>
	</div>

	<div class="col">
		<table class="table table-striped table-sm">
			<tbody id="df7dtable">
			{% for item in data["ecudata_df7d.json"]["ecudata"] %}
				{% if item["name"] not in excludes %}
					<tr>
						<td>{{ item['name'] | safe }}</td>
						<td class="ecudata_{{ item['id'] | safe }}">&nbsp;</td>
						<!--
						<td style="width: 30%"><div class="spark-container"><canvas id="{{ item['id'] | safe }}_ecuspark" title="{{ item['name'] | safe }}" class="ecuspark" height="30"></canvas></div></td>
						-->
					</tr>
				{% endif %}
			{% endfor %}
			</tbody>
		</table>
	</div>
</div>
