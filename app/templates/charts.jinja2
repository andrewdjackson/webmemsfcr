{% set excludes = ['_7Dx00_Time', '_80x00_Time', '_7D_RawData', '_80_RawData', '_80x04_AmbientTemp', '_80x06_FuelTemp', '_80x0B_AirconSwitch', 'CoolantTempSensorFault', 'FuelPumpCircuitFault', 'IntakeAirTempSensorFault', 'ThrottlePotCircuitFault'] %}

{% for item in data["ecudata_df80.json"]["ecudata"] %}
	{% if item["id"] not in excludes %}
		<div class="position-relative">
			<div class="chart-container"><canvas title="{{ item['name'] | safe }}" class="ecuchart {{ item['id'] | safe }}" height="150"></canvas></div>
			<div class="position-absolute top-50 end-0 m-3 ecudata-label">{{ item['name'] | safe }}</div>
			<div class="position-absolute bottom-0 end-0 m-3 ecudata_{{ item['id'] | safe }}">0</div>
		</div>
	{% endif %}
{% endfor %}

{% for item in data["ecudata_df7d.json"]["ecudata"] %}
	{% if item["id"] not in excludes  %}
		<div class="position-relative">
			<div class="chart-container"><canvas title="{{ item['name'] | safe }}" class="ecuchart {{ item['id'] | safe }}" height="150"></canvas></div>
			<div class="position-absolute top-50 end-0 m-3 ecudata-label">{{ item['name'] | safe }}</div>
			<div class="position-absolute bottom-0 end-0 m-3 ecudata_{{ item['id'] | safe }}">0</div>
		</div>
	{% endif %}
{% endfor %}

